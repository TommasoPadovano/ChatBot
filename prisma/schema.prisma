// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "file:./dev.db"
}

model Bot {
  id         Int      @id @default(autoincrement())
  name       String
  brains     String[] 
  mouths     String[] 
  history    String?
  image      String?
  status     String?  @default("inactive")
}